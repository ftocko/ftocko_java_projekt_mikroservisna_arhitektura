package org.foi.nwtis.ftocko.aplikacija_1;

import lombok.Getter;
import lombok.Setter;
import lombok.NonNull;
import lombok.AllArgsConstructor;

/**
 * Klasa za aerodrom.
 */
/*@AllArgsConstructor()*/
public class Aerodrom {
	
	/** kod aerodroma. */
	@Getter
    @Setter 
    @NonNull 
    String icao;
    
    /** naziv aerodroma. */
    @Getter
    @Setter 
    @NonNull 
    String naziv;
    
    /** gps geog.širina aerodroma. */
    @Getter
    @Setter 
    @NonNull 
    String gpsGS;
    
    /** gps geog.dužina aerodroma. */
    @Getter
    @Setter 
    @NonNull 
    String gpsGD;
    
    /**
     * Konstruktor za instanciranje objekta klase Aerodrom.
     *
     * @param icao kod aerodroma
     * @param naziv naziv aerodroma
     * @param gpsGS gps geog.širina aerodroma
     * @param gpsGD gps geog.dužina aerodroma
     */
    public Aerodrom(@NonNull String icao, @NonNull String naziv, @NonNull String gpsGS, @NonNull String gpsGD) {
		super();
		this.icao = icao;
		this.naziv = naziv;
		this.gpsGS = gpsGS;
		this.gpsGD = gpsGD;
	}
}
